{
	"css.validate": false,
	"editor.autoIndent": true,
	"editor.find.autoFindInSelection": true,
	"editor.detectIndentation": false,
	"editor.dragAndDrop": true,
	"editor.fontSize": 14,
	"editor.formatOnPaste": false,
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"editor.renderIndentGuides": true,
	"editor.selectionHighlight": true,
	"editor.occurrencesHighlight": true,
	"editor.insertSpaces": false,
	"editor.minimap.enabled": false,
	"editor.minimap.renderCharacters": true,
	"editor.mouseWheelZoom": true,
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.quickSuggestionsDelay": 0,
	"editor.showFoldingControls": "mouseover",
	"editor.tabSize": 4,
	"editor.useTabStops": true,
	"editor.wordWrap": "off",
	"emmet.triggerExpansionOnTab": true,
	"emmet.showAbbreviationSuggestions": false,
	"extensions.autoUpdate": true,
	"files.associations": {
		"*.vue": "html"
	},
	"workbench.colorCustomizations": {
		"editor.selectionHighlightBackground": "#99CCFF75",
		"editorOverviewRuler.selectionHighlightForeground": "#02A1FDD2",
		"editorOverviewRuler.findMatchForeground": "#02A1FDD2",
		"editorOverviewRuler.wordHighlightForeground": "#02A1FDD2",
		// "editorOverviewRuler.addedForeground": "#02A1FDD2",
		// "editorOverviewRuler.commonContentForeground": "#02A1FDD2",
		// "editorOverviewRuler.currentContentForeground": "#02A1FDD2",
		// "editorOverviewRuler.wordHighlightStrongForeground": "#02A1FDD2",
		// "editorOverviewRuler.rangeHighlightForeground": "#02A1FDD2",
		// "editorOverviewRuler.incomingContentForeground": "#02A1FDD2",
		// "editorOverviewRuler.infoForeground": "#02A1FDD2",
		// "editorRuler.foreground": "#02A1FDD2",
		// "peekViewResult.matchHighlightBackground": "#02A1FDD2",
		// "editorOverviewRuler.modifiedForeground": "#02A1FDD2",
		"editor.findMatchBackground": "#02A5FD67",
		"editor.findMatchHighlightBackground": "#02A5FD67"
	},
	"files.autoSave": "off",
	"git.confirmSync": false,
	"git.countBadge": "off",
	"git.enabled": false,
	"html.format.endWithNewline": true,
	"html.format.wrapAttributes": "auto",
	"html.format.wrapLineLength": 0,
	"html.suggest.angular1": false,
	"html.suggest.ionic": false,
	"javascript.format.enable": true,
	"javascript.format.placeOpenBraceOnNewLineForControlBlocks": false,
	"javascript.format.placeOpenBraceOnNewLineForFunctions": false,
	"path-autocomplete.extensionOnImport": true,
	"path-autocomplete.triggerOutsideStrings": true,
	"path-autocomplete.enableFolderTrailingSlash": true,
	"path-autocomplete.excludedItems": {
		"**/*.js": {
			"when": "**/*.ts"
		}, // ignore js files if i'm inside a ts file
		"**/*.map": {
			"when": "**"
		}, // always ignore *.map files
		"**/{.git,node_modules}": {
			"when": "**"
		} // always ignore .git and node_modules folders
	},
	"tsimporter.showNotifications": false,
	"tsimporter.noStatusBar": true,
	"tsimporter.preferRelative": true,
	"typescript.referencesCodeLens.enabled": false,
	"typescript.useCodeSnippetsOnMethodSuggest": false,
	//"vetur.validation.template": false,
	"vsicons.presets.angular": false,
	"vsicons.projectDetection.autoReload": true,
	"window.zoomLevel": 1,
	"workbench.colorTheme": "JSFiddle-like-syntax-vscode",
	"workbench.editor.tabCloseButton": "off",
	"workbench.iconTheme": "vscode-icons",
	"vsicons.dontShowNewVersionMessage": true,
	"terminal.integrated.shell.windows": "C:\\WINDOWS\\Sysnative\\cmd.exe"
}